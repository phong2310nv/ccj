    "@hookform/resolvers": "^5.2.2",
    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4",
    "@mui/material-nextjs": "^7.3.5",
    "@svgr/webpack": "^8.1.0",
    "axios": "^1.13.1",
    "dayjs": "^1.11.18",
    "next": "15.5.2",
    "notistack": "^3.0.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.66.0",
    "swiper": "^12.0.3",
    "yup": "^1.7.1"
