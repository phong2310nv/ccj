{
  "packageManager": "yarn@1.22.19",
  "engines": {
    "node": ">=20.0.0",
    "yarn": ">=1.22.0"
  },
  "name": "ccj",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
